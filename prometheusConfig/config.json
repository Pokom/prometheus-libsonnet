{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/prometheus/prometheus/config/config",
  "$ref": "#/$defs/Config",
  "$defs": {
    "AlertingConfig": {
      "properties": {
        "alert_relabel_configs": {
          "items": {
            "$ref": "#/$defs/Config"
          },
          "type": "array"
        },
        "alertmanagers": {
          "$ref": "#/$defs/AlertmanagerConfigs"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "AlertmanagerConfig": {
      "properties": {
        "HTTPClientConfig": {
          "$ref": "#/$defs/HTTPClientConfig"
        },
        "scheme": {
          "type": "string"
        },
        "path_prefix": {
          "type": "string"
        },
        "timeout": {
          "type": "integer"
        },
        "api_version": {
          "type": "string"
        },
        "relabel_configs": {
          "items": {
            "$ref": "#/$defs/Config"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "HTTPClientConfig",
        "api_version"
      ]
    },
    "AlertmanagerConfigs": {
      "items": {
        "$ref": "#/$defs/AlertmanagerConfig"
      },
      "type": "array"
    },
    "Authorization": {
      "properties": {
        "type": {
          "type": "string"
        },
        "credentials": {
          "type": "string"
        },
        "credentials_file": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "BasicAuth": {
      "properties": {
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "password_file": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "username"
      ]
    },
    "Config": {
      "properties": {
        "global": {
          "$ref": "#/$defs/GlobalConfig"
        },
        "alerting": {
          "$ref": "#/$defs/AlertingConfig"
        },
        "rule_files": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "scrape_config_files": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "scrape_configs": {
          "items": {
            "$ref": "#/$defs/ScrapeConfig"
          },
          "type": "array"
        },
        "storage": {
          "$ref": "#/$defs/StorageConfig"
        },
        "tracing": {
          "$ref": "#/$defs/TracingConfig"
        },
        "remote_write": {
          "items": {
            "$ref": "#/$defs/RemoteWriteConfig"
          },
          "type": "array"
        },
        "remote_read": {
          "items": {
            "$ref": "#/$defs/RemoteReadConfig"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "global"
      ]
    },
    "ExemplarsConfig": {
      "properties": {
        "max_exemplars": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "GlobalConfig": {
      "properties": {
        "scrape_interval": {
          "type": "integer"
        },
        "scrape_timeout": {
          "type": "integer"
        },
        "evaluation_interval": {
          "type": "integer"
        },
        "query_log_file": {
          "type": "string"
        },
        "external_labels": {
          "$ref": "#/$defs/Labels"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "HTTPClientConfig": {
      "properties": {
        "basic_auth": {
          "$ref": "#/$defs/BasicAuth"
        },
        "authorization": {
          "$ref": "#/$defs/Authorization"
        },
        "oauth2": {
          "$ref": "#/$defs/OAuth2"
        },
        "bearer_token": {
          "type": "string"
        },
        "bearer_token_file": {
          "type": "string"
        },
        "tls_config": {
          "$ref": "#/$defs/TLSConfig"
        },
        "follow_redirects": {
          "type": "boolean"
        },
        "enable_http2": {
          "type": "boolean"
        },
        "proxy_url": {
          "$ref": "#/$defs/URL"
        },
        "no_proxy": {
          "type": "string"
        },
        "proxy_from_environment": {
          "type": "boolean"
        },
        "proxy_connect_header": {
          "$ref": "#/$defs/Header"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "follow_redirects",
        "enable_http2"
      ]
    },
    "Header": {
      "patternProperties": {
        ".*": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "type": "object"
    },
    "Label": {
      "properties": {
        "Name": {
          "type": "string"
        },
        "Value": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "Name",
        "Value"
      ]
    },
    "LabelSet": {
      "patternProperties": {
        ".*": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "Labels": {
      "items": {
        "$ref": "#/$defs/Label"
      },
      "type": "array"
    },
    "MetadataConfig": {
      "properties": {
        "send": {
          "type": "boolean"
        },
        "send_interval": {
          "type": "integer"
        },
        "max_samples_per_send": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "send",
        "send_interval"
      ]
    },
    "OAuth2": {
      "properties": {
        "client_id": {
          "type": "string"
        },
        "client_secret": {
          "type": "string"
        },
        "client_secret_file": {
          "type": "string"
        },
        "scopes": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "token_url": {
          "type": "string"
        },
        "endpoint_params": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "tls_config": {
          "$ref": "#/$defs/TLSConfig"
        },
        "proxy_url": {
          "$ref": "#/$defs/URL"
        },
        "no_proxy": {
          "type": "string"
        },
        "proxy_from_environment": {
          "type": "boolean"
        },
        "proxy_connect_header": {
          "$ref": "#/$defs/Header"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "client_id",
        "client_secret",
        "client_secret_file",
        "token_url"
      ]
    },
    "QueueConfig": {
      "properties": {
        "capacity": {
          "type": "integer"
        },
        "max_shards": {
          "type": "integer"
        },
        "min_shards": {
          "type": "integer"
        },
        "max_samples_per_send": {
          "type": "integer"
        },
        "batch_send_deadline": {
          "type": "integer"
        },
        "min_backoff": {
          "type": "integer"
        },
        "max_backoff": {
          "type": "integer"
        },
        "retry_on_http_429": {
          "type": "boolean"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "RemoteReadConfig": {
      "properties": {
        "url": {
          "$ref": "#/$defs/URL"
        },
        "remote_timeout": {
          "type": "integer"
        },
        "headers": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "read_recent": {
          "type": "boolean"
        },
        "name": {
          "type": "string"
        },
        "HTTPClientConfig": {
          "$ref": "#/$defs/HTTPClientConfig"
        },
        "required_matchers": {
          "$ref": "#/$defs/LabelSet"
        },
        "filter_external_labels": {
          "type": "boolean"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "url",
        "HTTPClientConfig"
      ]
    },
    "RemoteWriteConfig": {
      "properties": {
        "url": {
          "$ref": "#/$defs/URL"
        },
        "remote_timeout": {
          "type": "integer"
        },
        "headers": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "write_relabel_configs": {
          "items": {
            "$ref": "#/$defs/Config"
          },
          "type": "array"
        },
        "name": {
          "type": "string"
        },
        "send_exemplars": {
          "type": "boolean"
        },
        "send_native_histograms": {
          "type": "boolean"
        },
        "HTTPClientConfig": {
          "$ref": "#/$defs/HTTPClientConfig"
        },
        "queue_config": {
          "$ref": "#/$defs/QueueConfig"
        },
        "metadata_config": {
          "$ref": "#/$defs/MetadataConfig"
        },
        "sigv4": {
          "$ref": "#/$defs/SigV4Config"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "url",
        "HTTPClientConfig"
      ]
    },
    "ScrapeConfig": {
      "properties": {
        "job_name": {
          "type": "string"
        },
        "honor_labels": {
          "type": "boolean"
        },
        "honor_timestamps": {
          "type": "boolean"
        },
        "params": {
          "$ref": "#/$defs/Values"
        },
        "scrape_interval": {
          "type": "integer"
        },
        "scrape_timeout": {
          "type": "integer"
        },
        "metrics_path": {
          "type": "string"
        },
        "scheme": {
          "type": "string"
        },
        "body_size_limit": {
          "type": "integer"
        },
        "sample_limit": {
          "type": "integer"
        },
        "target_limit": {
          "type": "integer"
        },
        "label_limit": {
          "type": "integer"
        },
        "label_name_length_limit": {
          "type": "integer"
        },
        "label_value_length_limit": {
          "type": "integer"
        },
        "HTTPClientConfig": {
          "$ref": "#/$defs/HTTPClientConfig"
        },
        "relabel_configs": {
          "items": {
            "$ref": "#/$defs/Config"
          },
          "type": "array"
        },
        "metric_relabel_configs": {
          "items": {
            "$ref": "#/$defs/Config"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "job_name",
        "honor_timestamps",
        "HTTPClientConfig"
      ]
    },
    "SigV4Config": {
      "properties": {
        "region": {
          "type": "string"
        },
        "access_key": {
          "type": "string"
        },
        "secret_key": {
          "type": "string"
        },
        "profile": {
          "type": "string"
        },
        "role_arn": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "StorageConfig": {
      "properties": {
        "tsdb": {
          "$ref": "#/$defs/TSDBConfig"
        },
        "exemplars": {
          "$ref": "#/$defs/ExemplarsConfig"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "TLSConfig": {
      "properties": {
        "ca_file": {
          "type": "string"
        },
        "cert_file": {
          "type": "string"
        },
        "key_file": {
          "type": "string"
        },
        "server_name": {
          "type": "string"
        },
        "insecure_skip_verify": {
          "type": "boolean"
        },
        "min_version": {
          "type": "integer"
        },
        "max_version": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "insecure_skip_verify"
      ]
    },
    "TSDBConfig": {
      "properties": {
        "OutOfOrderTimeWindow": {
          "type": "integer"
        },
        "out_of_order_time_window": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "OutOfOrderTimeWindow"
      ]
    },
    "TracingConfig": {
      "properties": {
        "client_type": {
          "type": "string"
        },
        "endpoint": {
          "type": "string"
        },
        "sampling_fraction": {
          "type": "number"
        },
        "insecure": {
          "type": "boolean"
        },
        "tls_config": {
          "$ref": "#/$defs/TLSConfig"
        },
        "headers": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "compression": {
          "type": "string"
        },
        "timeout": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "URL": {
      "properties": {
        "Scheme": {
          "type": "string"
        },
        "Opaque": {
          "type": "string"
        },
        "User": {
          "$ref": "#/$defs/Userinfo"
        },
        "Host": {
          "type": "string"
        },
        "Path": {
          "type": "string"
        },
        "RawPath": {
          "type": "string"
        },
        "OmitHost": {
          "type": "boolean"
        },
        "ForceQuery": {
          "type": "boolean"
        },
        "RawQuery": {
          "type": "string"
        },
        "Fragment": {
          "type": "string"
        },
        "RawFragment": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "Scheme",
        "Opaque",
        "User",
        "Host",
        "Path",
        "RawPath",
        "OmitHost",
        "ForceQuery",
        "RawQuery",
        "Fragment",
        "RawFragment"
      ]
    },
    "Userinfo": {
      "properties": {},
      "additionalProperties": false,
      "type": "object"
    },
    "Values": {
      "patternProperties": {
        ".*": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "type": "object"
    }
  }
}